<header>
    <div class="header">
        <!--Logo-->
        <div class="header__container">
            <div class="logo">
                <a href="/"><img class="header__logo" src="/img/Logo/blancV2ojo.png" alt="Logo de Oracula"></a>
            </div>
            <!--Hamburguesita-->
            <div class="Burguer">
                <button><i id="Burguer_icon" class="iconify" data-icon="eva:menu-fill"></i></button>
            </div>
            <!--Busqueda-->
            <form class="Busqueda" action="#" method="GET">
                <input class="Busqueda_input" type="search">
                <div>
                    <button class="Busqueda_button" class="submit">
                        <i class="iconify" data-icon="pepicons:loop"></i>
                    </button>
                </div>
            </form>
            <!-- Canastita Mobile-->
            <nav class="basket">
                <button><i class="iconify" data-icon="mdi:basket"></i></button>
            </nav>
            <!--Secciones categorias-->
            <nav class="secciones">
                <ul class="secciones__lista">
                    <li>
                        <button class="secciones__elemento">
                            <i class="iconify secciones__icon" data-icon="fa6-solid:explosion"></i>
                            Magic Pass
                        </button>
                    </li>
                    <li>
                        <button class="secciones__elemento">
                            <i class="iconify secciones__icon" data-icon="fa6-solid:hamsa"></i>
                            Géneros
                        </button>
                    </li>
                    <li>
                        <button class="secciones__elemento">
                            <i class="iconify secciones__icon" data-icon="mdi:bottle-tonic-outline"></i>
                            Categorías
                        </button>
                    </li>
                    <li>
                        <button class="secciones__elemento">
                            <i class="fa-solid fa-toolbox secciones__icon"></i>
                            Admin
                        </button>
                    </li>
                </ul>
            </nav>
            <!--Botones-->
            <nav class="botonesSuperiores">
                <ul class="botonesSuperiores__lista">
                    <li class="botonesSuperiores__elemento">
                        <button class="botonesSuperiores__boton">
                            <i class="iconify secciones__icon" data-icon="fa:search"></i>
                        </button>
                    </li>
                    <li class="botonesSuperiores__elemento">
                        <button class="botonesSuperiores__boton">
                            <a href="/product/product"><i class="iconify secciones__icon"
                                    data-icon="pepicons:bookmark-print"></i></a>
                        </button>
                    </li>
                    <li class="botonesSuperiores__elemento">
                        <button class="botonesSuperiores__boton">
                            <a href="/product/cart"><i class="iconify secciones__icon" data-icon="mdi:basket"></i></a>
                        </button>
                    </li>
                    <li class="botonesSuperiores__elemento">
                        <button class="botonesSuperiores_Login">
                            <a href="/user/login">
                                <div class="botonesSuperiores_Login--texto">¡Ingresá!</div>
                            </a>
                            <i id="logoEntrar2" class="iconify  secciones__icon"
                                data-icon="arcticons:monument-valley"></i>
                            <i id="logoEntrar1" class="iconify  secciones__icon" data-icon="bi:door-open-fill"
                                data-flip="horizontal"></i>
                        </button>
                    </li>
                </ul>
            </nav>
        </div>
        <nav class="menu__container">
            <ul class="menu menu--generos">
                <li class="menu__item">
                </li>
            </ul>
            <ul class="menu menu--categorias">
                <li class="menu__item">
                    <a class="menu__link" href="LINK">
                        <img class="menu__imagen" src="/img/productos/literatura/AliciaIlustrado.jpg" alt="Imagen de Literatura">
                        <h4>Literatura</h4>   
                    </a>
                </li>
                <li class="menu__item">
                    <a class="menu__link" href="LINK">
                        <img class="menu__imagen" src="/img/productos/juegos/stardewValley.png" alt="">
                        <h4>Juegos</h4>
                    </a>
                </li>
                <li class="menu__item">
                    <a class="menu__link" href="LINK">
                        <img class="menu__imagen" src="/img/productos/baratijas/mundoDisco.jpg" alt="">
                        <h4>Baratijas</h4>
                    </a>
                </li>
                <li class="menu__item">
                    <a class="menu__link" href="LINK">
                        <img class="menu__imagen" src="/img/productos/multimedia/lordOfRingsSound.jpg" alt="">
                        <h4>Audiovisuales</h4>
                    </a>
                </li>
            </ul>
            <ul class="menu menu--admin">
                <li class="menu__item">
                </li>
            </ul>
        </nav>
    </div>
    <!--Relleno de header-->
    <div class="header__relleno"></div>
</header>